file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Stats$(N):,
	PORT=$(CAM_PORT).Stats$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_SRC),
	NDARRAY_ADDR=0
	}
}

file NDStats.template
{
	{
	P=$(CAM):,
	R=Stats$(N):,
	PORT=$(CAM_PORT).Stats$(N),
	ADDR=0,
	TIMEOUT=1
	}
}

