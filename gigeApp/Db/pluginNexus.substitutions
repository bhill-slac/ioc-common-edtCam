file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Nexus$(N):,
	PORT=FileNexus$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_PORT),
	NDARRAY_ADDR=0
	}
}

