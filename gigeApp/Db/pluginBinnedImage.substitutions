file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=BinnedImage$(N):,
	PORT=BinnedImage$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_SRC),
	NDARRAY_ADDR=0
	}
}

file NDStdArrays.template
{
	{
	P=$(CAM):,
	R=BinnedImage$(N):,
	PORT=BinnedImage$(N),
	TYPE=Int8,
	FTVL=UCHAR,
	NELEMENTS=$(BINNED_IMAGE_NELM),
	ADDR=0,
	TIMEOUT=1
	}
}

