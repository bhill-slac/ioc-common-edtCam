file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Thumbnail$(N):,
	PORT=$(CAM_PORT).Thumbnail$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_SRC),
	NDARRAY_ADDR=0
	}
}

file NDStdArrays.template
{
	{
	P=$(CAM):,
	R=Thumbnail$(N):,
	PORT=$(CAM_PORT).Thumbnail$(N),
	TYPE=Int8,
	FTVL=UCHAR,
	NELEMENTS=$(THUMBNAIL_NELM),
	ADDR=0,
	TIMEOUT=1
	}
}

