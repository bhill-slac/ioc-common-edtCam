file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Magick$(N):,
	PORT=$(CAM_PORT).FileMagick$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_SRC),
	NDARRAY_ADDR=0
	}
}

file NDFile.template
{
	{
	P=$(CAM):,
	R=Magick$(N):,
	PORT=$(CAM_PORT).FileMagick$(N),
	ADDR=0,
	TIMEOUT=1
	}
}

file NDFileMagick.template
{
	{
	P=$(CAM):,
	R=Magick$(N):,
	PORT=$(CAM).FileMagick$(N),
	ADDR=0,
	TIMEOUT=1
	}
}

