file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Magick$(N):,
	PORT=FileMagick$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_PORT),
	NDARRAY_ADDR=0
	}
}

file NDFile.template
{
	{
	P=$(CAM):,
	R=Magick$(N):,
