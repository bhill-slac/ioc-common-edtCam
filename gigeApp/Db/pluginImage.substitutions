#
# pluginImage.substitutions
#

file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Image$(N):,
	PORT=Image$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_SRC),
	NDARRAY_ADDR=0,
	BIT_DEPTH=$(IMAGE_BIT_DEPTH),
	}
}

file NDStdArrays.template
{
	{
	P=$(CAM):,
	R=Image$(N):,
	PORT=Image$(N),
	TYPE=Int16,
	FTVL=SHORT,
	NELEMENTS=$(IMAGE_NELM),
	ADDR=0,
	TIMEOUT=1
	}
}

